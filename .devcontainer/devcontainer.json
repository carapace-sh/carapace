{
  "name": "carapace",
  "image": "ghcr.io/carapace-sh/carapace",
  "settings": {
    "terminal.integrated.shell.linux": "/usr/local/bin/elvish"
  },
  "extensions": [
    "golang.Go"
  ],
  "containerEnv": {
    "TARGET": "example"
  },
  "onCreateCommand": [
    "sh",
    "-c",
    "cd example && go install ."
  ]
}
